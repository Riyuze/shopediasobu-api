{
    "version": 2,
    "builds": [
        {
            "src": "./build/server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["build/**"] },
            "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./build/server.js"
        }
    ]
}